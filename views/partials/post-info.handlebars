<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title"><a href = '/post/{{id}}'>{{title}}</a></h5>
    <p class="card-text" id = "card-details">{{user.username}} | {{format_date date_created}}</p>
    <br>
    <p class="card-text">{{content}}</p>
  </div>
</div>
